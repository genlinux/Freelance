<h1>New article</h1>
<% form_for(@article) do |f| %>
<%= f.error_messages %>
<p>
    <%=f.label :status %>
    <br/>
    <%=f.radio_button :status,:checkbox%>
</p>
<p>
    <%=f.label :published_at %>
    <br/>
    <%=f.datetime_select :published_at %>
</p>
<p>
    <%= f.submit 'Create' %>
</p>
<% end %>
<%= link_to 'Back', articles_path %>